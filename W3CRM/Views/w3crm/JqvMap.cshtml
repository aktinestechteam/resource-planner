@{
	ViewData["Title"] = "Jqv Map";
}
<!--**********************************
	Content body start
***********************************-->
<div class="content-body">
    <div class="page-titles">
        <ol class="breadcrumb">
            <li class="breadcrumb-item active"><a href="javascript:void(0)">Plugins</a></li>
            <li class="breadcrumb-item active"><a href="javascript:void(0)">Jqv Map</a></li>
        </ol>
    </div>
    <div class="container-fluid">


        <!-- row -->
        <!-- Vectormap -->
        <div class="row">
            <div class="col-lg-6">
                <div class="card">
                    <div class="card-header">
                        <h4 class="card-title">World Map</h4>
                    </div>
                    <div class="card-body">
                        <div id="world-map"></div>
                    </div>
                </div>
            </div>
            <div class="col-lg-6">
                <div class="card">
                    <div class="card-header">
                        <h4 class="card-title">USA</h4>
                    </div>
                    <div class="card-body">
                        <div id="usa" style="height: 450px;"></div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<!--**********************************
	Content body end
***********************************-->
@section w3crm_style
	{
	<!-- Vectormap -->
    <link href="~/w3crm/vendor/jqvmap/css/jqvmap.min.css" rel="stylesheet">
}

@section w3crm_script
	{
	<!--**********************************
		Scripts
	***********************************-->
    <!-- Required vendors -->
    <script src="~/w3crm/vendor/global/global.min.js"></script>

    <!-- Vectormap -->
    <script src="~/w3crm/vendor/jqvmap/js/jquery.vmap.min.js"></script>
    <script src="~/w3crm/vendor/jqvmap/js/jquery.vmap.world.js"></script>
    <script src="~/w3crm/vendor/jqvmap/js/jquery.vmap.usa.js"></script>
    <script src="~/w3crm/vendor/bootstrap-select/dist/js/bootstrap-select.min.js"></script>

    <!-- All init script -->
    <script src="~/w3crm/js/plugins-init/jqvmap-init.js"></script>
    <script src="~/w3crm/js/custom.js"></script>
    <script src="~/w3crm/js/deznav-init.js"></script>
    <script src="~/w3crm/js/demo.js"></script>
    <script src="~/w3crm/js/styleSwitcher.js"></script>
}
