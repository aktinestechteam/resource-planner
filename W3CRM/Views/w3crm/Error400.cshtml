@{
	ViewData["Title"] = "Error 400";
	Layout = "~/Views/Shared/_LayoutW3CRMBlank.cshtml";
}
<!--**********************************
	Content body start
***********************************-->
<div class="container ">
	<div class="row justify-content-center h-100 align-items-center">
		<div class="col-md-6">
			<div class="error-page">
				<div class="error-inner text-center">
					<div class="dz-error" data-text="400">400</div>
					<h2 class="error-head">We are sorry. But the page you are looking for cannot be found.</h2>
					<a href="@Url.Action("Index", "w3crm")" class="btn btn-secondary">BACK TO HOMEPAGE</a>
				</div>
			</div>
		</div>
	</div>
</div>
<!--**********************************
	Content body end
***********************************-->
